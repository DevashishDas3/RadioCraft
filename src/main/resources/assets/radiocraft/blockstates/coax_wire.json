{
  "multipart": [
    {
      "apply": {
        "model": "radiocraft:block/wire_middle"
      }
    },
    {
      "apply": {
        "model": "radiocraft:block/wire"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "radiocraft:block/wire",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "radiocraft:block/wire",
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "radiocraft:block/wire",
        "y": 270
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "radiocraft:block/wire",
        "x": 270
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "radiocraft:block/wire",
        "x": 90
      },
      "when": {
        "down": "true"
      }
    }
  ]
}